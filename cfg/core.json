                "action": "home"
            }
        },
        "helper": {
            "Demo": {
                "name": "Crypto",
                "module": "app",
                "path": "service"
            },
            "dao": {
                "name": "DAOSequelize",
                "path": "service/DAO",
                "dependency": {
                    "helper": "helper"
                }
            }
        }
    }
}