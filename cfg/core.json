            ]
        },
        "route": {
            "/": {
                "method": "get",
                "module": "app",
                "controller": "DefaultController",
                "action": "home"
            }
        },
        "helper": {
            "Demo": {
                "name": "Cripto",
                "module": "app",
                "path": "service"
            },
            "dao": {
                "name": "DAOSequelize",
                "path": "service/DAO",
                "dependency": {
                    "helper": "helper"
                }
            }
        }
    },
    "test": {
        "port": 3031,
        "host": "localhost",
        "protocol": "http",
        "log": 0,
