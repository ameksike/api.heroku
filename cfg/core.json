{
    "development": {
        "port": 3031,
        "host": "localhost",
        "protocol": "http",
        "log": 1,
        "module": {
            "load": [
                "etl",
                "app",
                {
                    "name": "ksmf",
                    "type": "lib",
                    "namespace": "task.Cron"
                }
            ]
        },
        "route": {
            "/": {
                "method": "get",
                "module": "app",
                "controller": "DefaultController",
                "action": "home"
            }
        },
        "helper": {
            "sequelize": {
                "name": "ksmf",
                "type": "lib",
                "namespace": "dao.Sequelize",
                "dependency": {
                    "helper": "helper"
                }
            },
            "dao": {
                "type": "alias",
                "source": "sequelize"
            },
            "error": {
                "name": "ksmf",
                "type": "lib",
                "namespace": "app.Error"
            },
            "logger": {
                "name": "ksmf",
                "type": "lib",
                "namespace": "app.Logger"
            }
        },
        "cron": [
            {
                "env": "SCHEDULE_RETRIVE_DATA",
                "value": "0/1 * * * *",
                "target": {
                    "type": "action",
                    "module": "etl",
                    "name": "LoginService",
                    "action": "dailyProcessing"
                }
            }
        ]
    },
    "production": {
