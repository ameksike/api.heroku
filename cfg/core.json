{
    "development": {
        "port": 3031,
        "host": "localhost",
        "protocol": "http",
        "log": 1,
        "module": {
            "load": [
                "etl",
                "app"
            ]
        },
        "route": {
            "/": {
                "method": "get",
                "module": "app",
                "controller": "DefaultController",
                "action": "home"
            }
        },
        "helper": {
            "Demo": {
                "name": "Crypto",
                "module": "app",
                "path": "service"
            },
            "DAOSequelize": {
                "path": "service/DAO",
                "dependency": {
                    "helper": "helper"
                }
            },
            "dao": {
                "type": "alias",
                "source": "DAOSequelize"
            }
        }
    },
    "production": {
        "port": 3000,
        "host": "localhost",
        "protocol": "http",
        "log": 1,
